<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" elementFormDefault="qualified" xmlns:xhtml="http://www.w3.org/1999/xhtml" xmlns:ssml="http://www.w3.org/2001/10/synthesis" xmlns:ev="http://www.w3.org/2001/xml-events" xmlns:mathml="http://www.w3.org/1998/Math/MathML" xmlns:epub="http://www.idpf.org/2007/ops" xmlns:xlink="http://www.w3.org/1999/xlink" xmlns:svg="http://www.w3.org/2000/svg">
  <xs:import namespace="http://www.idpf.org/2007/ops" schemaLocation="epub.xsd"/>
  <xs:import namespace="http://www.w3.org/1998/Math/MathML" schemaLocation="epub-mathml3.xsd"/>
  <xs:import namespace="http://www.w3.org/1999/xhtml" schemaLocation="epub-nav-30.xsd"/>
  <xs:import namespace="http://www.w3.org/1999/xlink" schemaLocation="svg-xlink-attrib.xsd"/>
  <xs:import namespace="http://www.w3.org/2000/svg" schemaLocation="svg.xsd"/>
  <xs:import namespace="http://www.w3.org/2001/10/synthesis" schemaLocation="epub-ssml-attrs.xsd"/>
  <xs:import namespace="http://www.w3.org/2001/xml-events" schemaLocation="ev.xsd"/>
  <xs:import namespace="http://www.w3.org/XML/1998/namespace" schemaLocation="xml.xsd"/>
  <!-- ##################################################################### -->
  <!--
                            (This only affects mixed-version modules; wholly HTML5
                             modules should simply be left out of the inclusion list.)
  -->
  <!--  (xml:base on elements other than <html>) -->
  <!-- ##################################################################### -->
  <xs:attributeGroup name="common.attr.anything">
    <xs:annotation>
      <xs:documentation> Wildcards                                                         #</xs:documentation>
    </xs:annotation>
    <xs:anyAttribute processContents="skip"/>
  </xs:attributeGroup>
  <xs:group name="common.elem.anything">
    <xs:annotation>
      <xs:documentation>Any element from any namespace</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:any processContents="skip"/>
    </xs:sequence>
  </xs:group>
  <xs:complexType name="common.inner.anything" mixed="true">
    <xs:annotation>
      <xs:documentation>Any content from any namespace</xs:documentation>
    </xs:annotation>
    <xs:group minOccurs="0" maxOccurs="unbounded" ref="common.elem.anything"/>
  </xs:complexType>
  <!-- ##################################################################### -->
  <xs:group name="common.elem.metadata">
    <xs:annotation>
      <xs:documentation> Common Element Classes                                            #</xs:documentation>
    </xs:annotation>
    <xs:choice>
      <xs:group ref="xhtml:link.elem"/>
      <xs:element ref="xhtml:style"/>
      <xs:group ref="xhtml:meta.name.elem"/>
      <xs:group ref="xhtml:meta.http-equiv.refresh.elem"/>
      <xs:group ref="xhtml:meta.http-equiv.default-style.elem"/>
      <xs:group ref="xhtml:meta.http-equiv.content-security-policy.elem"/>
      <xs:group ref="xhtml:meta.http-equiv.x-ua-compatible.elem"/>
      <xs:choice>
        <xs:group ref="xhtml:meta.charset.elem"/>
        <xs:group ref="xhtml:meta.http-equiv.content-type.elem"/>
      </xs:choice>
      <xs:group ref="xhtml:script.elem"/>
      <xs:group ref="xhtml:meta.itemprop.elem"/>
      <xs:element ref="xhtml:template"/>
      <xs:group ref="xhtml:link.rdfa.elem.metadata"/>
      <xs:group ref="xhtml:meta.property.elem"/>
      <xs:element ref="epub:trigger"/>
    </xs:choice>
  </xs:group>
  <xs:group name="common.elem.phrasing">
    <xs:annotation>
      <xs:documentation>Phrase Elements</xs:documentation>
    </xs:annotation>
    <xs:choice>
      <xs:group ref="xhtml:link.elem"/>
      <xs:group ref="xhtml:a.elem.phrasing"/>
      <xs:element ref="xhtml:em"/>
      <xs:element ref="xhtml:strong"/>
      <xs:element ref="xhtml:small"/>
      <xs:element ref="xhtml:mark"/>
      <xs:element ref="xhtml:abbr"/>
      <xs:element ref="xhtml:dfn"/>
      <xs:element ref="xhtml:i"/>
      <xs:element ref="xhtml:b"/>
      <xs:element ref="xhtml:s"/>
      <xs:element ref="xhtml:u"/>
      <xs:element ref="xhtml:code"/>
      <xs:element ref="xhtml:var"/>
      <xs:element ref="xhtml:samp"/>
      <xs:element ref="xhtml:kbd"/>
      <xs:element ref="xhtml:sup"/>
      <xs:element ref="xhtml:sub"/>
      <xs:element ref="xhtml:q"/>
      <xs:element ref="xhtml:cite"/>
      <xs:element ref="xhtml:span"/>
      <xs:element ref="xhtml:bdo"/>
      <xs:element ref="xhtml:bdi"/>
      <xs:element ref="xhtml:br"/>
      <xs:element ref="xhtml:wbr"/>
      <xs:group ref="xhtml:ins.elem.phrasing"/>
      <xs:group ref="xhtml:del.elem.phrasing"/>
      <xs:element ref="xhtml:img"/>
      <xs:element ref="xhtml:picture"/>
      <xs:element ref="xhtml:embed"/>
      <xs:group ref="xhtml:object.elem.phrasing"/>
      <xs:element ref="xhtml:iframe"/>
      <xs:group ref="xhtml:map.elem.phrasing"/>
      <xs:element ref="xhtml:area"/>
      <xs:element ref="xhtml:ruby"/>
      <xs:group ref="xhtml:video.elem.phrasing"/>
      <xs:group ref="xhtml:audio.elem.phrasing"/>
      <xs:group ref="xhtml:script.elem"/>
      <xs:group ref="xhtml:input.elem"/>
      <xs:element ref="xhtml:textarea"/>
      <xs:element ref="xhtml:select"/>
      <xs:group ref="xhtml:button.elem"/>
      <xs:element ref="xhtml:label"/>
      <xs:element ref="xhtml:output"/>
      <xs:element ref="xhtml:datalist"/>
      <xs:element ref="xhtml:progress"/>
      <xs:group ref="xhtml:canvas.elem.phrasing"/>
      <xs:group ref="xhtml:time.elem"/>
      <xs:group ref="xhtml:time.datetime.elem"/>
      <xs:element ref="xhtml:data"/>
      <xs:element ref="xhtml:meter"/>
      <xs:group ref="xhtml:link.elem.phrasing"/>
      <xs:group ref="xhtml:meta.itemprop.elem"/>
      <xs:element ref="xhtml:template"/>
      <xs:any namespace="http://n.validator.nu/custom-elements/" processContents="skip"/>
      <xs:group ref="xhtml:link.rdfa.elem.phrasing"/>
      <xs:group ref="xhtml:meta.property-no-name.elem"/>
      <xs:group ref="epub:epub.switch.phrasing"/>
      <xs:element ref="mathml:math"/>
      <xs:element ref="svg:svg"/>
    </xs:choice>
  </xs:group>
  <xs:group name="common.elem.flow">
    <xs:annotation>
      <xs:documentation>Prose Elements</xs:documentation>
    </xs:annotation>
    <xs:choice>
      <xs:group ref="common.elem.phrasing"/>
      <xs:group ref="xhtml:link.elem"/>
      <xs:group ref="xhtml:a.elem.flow"/>
      <xs:element ref="xhtml:p"/>
      <xs:element ref="xhtml:hr"/>
      <xs:element ref="xhtml:pre"/>
      <xs:element ref="xhtml:ul"/>
      <xs:group ref="xhtml:ol.elem"/>
      <xs:element ref="xhtml:dl"/>
      <xs:group ref="xhtml:div.elem"/>
      <xs:element ref="xhtml:h1"/>
      <xs:element ref="xhtml:h2"/>
      <xs:element ref="xhtml:h3"/>
      <xs:element ref="xhtml:h4"/>
      <xs:element ref="xhtml:h5"/>
      <xs:element ref="xhtml:h6"/>
      <xs:element ref="xhtml:hgroup"/>
      <xs:element ref="xhtml:address"/>
      <xs:element ref="xhtml:blockquote"/>
      <xs:element ref="xhtml:section"/>
      <xs:group ref="xhtml:nav.elem"/>
      <xs:element ref="xhtml:article"/>
      <xs:element ref="xhtml:aside"/>
      <xs:element ref="xhtml:header"/>
      <xs:element ref="xhtml:footer"/>
      <xs:element ref="xhtml:main"/>
      <xs:group ref="xhtml:ins.elem.flow"/>
      <xs:group ref="xhtml:del.elem.flow"/>
      <xs:group ref="xhtml:object.elem.flow"/>
      <xs:group ref="xhtml:map.elem.flow"/>
      <xs:group ref="xhtml:video.elem.flow"/>
      <xs:group ref="xhtml:audio.elem.flow"/>
      <xs:element ref="xhtml:figure"/>
      <xs:element ref="xhtml:table"/>
      <xs:element ref="xhtml:form"/>
      <xs:element ref="xhtml:fieldset"/>
      <xs:element ref="xhtml:dialog"/>
      <xs:element ref="xhtml:menu"/>
      <xs:group ref="xhtml:canvas.elem.flow"/>
      <xs:element ref="xhtml:details"/>
      <xs:any namespace="http://n.validator.nu/custom-elements/" processContents="skip"/>
      <xs:element ref="epub:trigger"/>
      <xs:group ref="epub:epub.switch.flow"/>
      <xs:group ref="xhtml:epub.nav"/>
    </xs:choice>
  </xs:group>
  <!-- ##################################################################### -->
  <xs:group name="common.inner.metadata">
    <xs:annotation>
      <xs:documentation> Common Content Models                                             #</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:group minOccurs="0" maxOccurs="unbounded" ref="common.elem.metadata"/>
    </xs:sequence>
  </xs:group>
  <xs:group name="common.inner.phrasing">
    <xs:annotation>
      <xs:documentation>Phrase Content</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:group minOccurs="0" maxOccurs="unbounded" ref="common.elem.phrasing"/>
    </xs:sequence>
  </xs:group>
  <xs:group name="common.inner.transparent.flow">
    <xs:annotation>
      <xs:documentation>Prose Content</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:group minOccurs="0" maxOccurs="unbounded" ref="common.elem.flow"/>
    </xs:sequence>
  </xs:group>
  <xs:group name="common.inner.flow">
    <xs:sequence>
      <xs:group minOccurs="0" maxOccurs="unbounded" ref="common.elem.flow"/>
    </xs:sequence>
  </xs:group>
  <!-- ##################################################################### -->
  <!-- ##################################################################### -->
  <!-- When updating, check <bdo> definition too -->
  <xs:attributeGroup name="common.attrs">
    <xs:annotation>
      <xs:documentation> Common Attributes                                                 #</xs:documentation>
    </xs:annotation>
    <xs:attributeGroup ref="common.attrs.basic"/>
    <xs:attributeGroup ref="common.attrs.i18n"/>
    <xs:attributeGroup ref="common.attrs.present"/>
    <xs:attributeGroup ref="common.attrs.other"/>
    <xs:attributeGroup ref="xhtml:common.attrs.microdata"/>
    <xs:attributeGroup ref="xhtml:common.attrs.rdfa"/>
  </xs:attributeGroup>
  <xs:attributeGroup name="common.attrs.basic">
    <xs:attribute name="id" type="common.data.id"/>
    <xs:attribute name="class" type="common.data.tokens"/>
    <xs:attribute name="title"/>
    <xs:attributeGroup ref="xml:base"/>
    <xs:attributeGroup ref="xml:space"/>
    <xs:attribute ref="epub:type"/>
  </xs:attributeGroup>
  <xs:attributeGroup name="common.attrs.id">
    <xs:attribute name="id" use="required" type="common.data.id"/>
  </xs:attributeGroup>
  <xs:attributeGroup name="common.attrs.class">
    <xs:attribute name="class" use="required" type="common.data.tokens"/>
  </xs:attributeGroup>
  <xs:attributeGroup name="common.attrs.title">
    <xs:attribute name="title" use="required"/>
  </xs:attributeGroup>
  <xs:attributeGroup name="common.attrs.base">
    <xs:attributeGroup ref="common.attrs.xmlbase"/>
  </xs:attributeGroup>
  <xs:attributeGroup name="common.attrs.xmlbase">
    <xs:attributeGroup ref="xml:base"/>
  </xs:attributeGroup>
  <xs:attributeGroup name="common.attrs.space">
    <xs:attributeGroup ref="common.attrs.xmlspace"/>
  </xs:attributeGroup>
  <xs:attributeGroup name="common.attrs.xmlspace">
    <xs:attributeGroup ref="xml:space"/>
  </xs:attributeGroup>
  <xs:attributeGroup name="common.attrs.i18n">
    <xs:attribute name="dir">
      <xs:simpleType>
        <xs:restriction base="xs:token">
          <xs:enumeration value="ltr"/>
          <xs:enumeration value="rtl"/>
          <xs:enumeration value="auto"/>
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
    <xs:attributeGroup ref="common.attrs.language"/>
    <xs:attribute name="translate">
      <xs:simpleType>
        <xs:restriction base="xs:token">
          <xs:enumeration value=""/>
          <xs:enumeration value="yes"/>
          <xs:enumeration value="no"/>
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
  </xs:attributeGroup>
  <xs:attributeGroup name="common.attrs.dir">
    <xs:attribute name="dir" use="required">
      <xs:simpleType>
        <xs:restriction base="xs:token">
          <xs:enumeration value="ltr"/>
          <xs:enumeration value="rtl"/>
          <xs:enumeration value="auto"/>
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
  </xs:attributeGroup>
  <!--
    This lang definition is a hack for environments where
    the HTML5 parser maps lang to xml:lang.
    Sameness check left to Schematron
  -->
  <xs:attributeGroup name="common.attrs.language">
    <xs:attributeGroup ref="xml:lang"/>
    <xs:attribute name="lang" type="common.data.langcode"/>
  </xs:attributeGroup>
  <xs:attributeGroup name="common.attrs.lang">
    <xs:attribute name="lang" use="required" type="common.data.langcode"/>
  </xs:attributeGroup>
  <xs:attributeGroup name="common.attrs.xmllang">
    <xs:attributeGroup ref="xml:lang"/>
  </xs:attributeGroup>
  <!--
    	common.attrs.language =
    		(	common.attrs.lang
    		|	common.attrs.xmllang
    		)
    	common.attrs.lang =
    		attribute lang {
    			common.data.langcode
    		} & HTMLonly
    	common.attrs.xmllang =
    		attribute xml:lang {
    			common.data.langcode
    		} & XMLonly
  -->
  <xs:attributeGroup name="common.attrs.translate">
    <xs:attribute name="translate" use="required">
      <xs:simpleType>
        <xs:restriction base="xs:token">
          <xs:enumeration value=""/>
          <xs:enumeration value="yes"/>
          <xs:enumeration value="no"/>
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
  </xs:attributeGroup>
  <xs:attributeGroup name="common.attrs.present">
    <xs:attribute name="style" type="xs:string"/>
    <xs:attribute name="tabindex" type="common.data.integer"/>
    <xs:attribute name="accesskey" type="common.data.keylabellist"/>
  </xs:attributeGroup>
  <xs:attributeGroup name="common.attrs.style">
    <xs:attribute name="style" use="required" type="xs:string"/>
  </xs:attributeGroup>
  <xs:attributeGroup name="common.attrs.tabindex">
    <xs:attribute name="tabindex" use="required" type="common.data.integer"/>
  </xs:attributeGroup>
  <!-- REVISIT move style to a module and bundle tabindex with ARIA -->
  <xs:attributeGroup name="common.attrs.accesskey">
    <xs:attribute name="accesskey" use="required" type="common.data.keylabellist"/>
  </xs:attributeGroup>
  <xs:attributeGroup name="common.attrs.other">
    <xs:attribute name="autofocus">
      <xs:simpleType>
        <xs:restriction base="xs:token">
          <xs:enumeration value="autofocus"/>
          <xs:enumeration value=""/>
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
    <xs:attribute name="inputmode" type="xs:string"/>
    <xs:attribute name="nonce" type="xs:string"/>
    <xs:attributeGroup ref="xhtml:common.attrs.scripting"/>
    <xs:attributeGroup ref="xhtml:common.attrs.interact"/>
    <xs:attributeGroup ref="aria.global"/>
    <xs:attribute name="is" type="common.data.custom.element.name"/>
    <xs:attribute name="slot"/>
    <xs:attribute ref="ssml:ph"/>
    <xs:attribute ref="ssml:alphabet"/>
  </xs:attributeGroup>
  <xs:attributeGroup name="common.attrs.autofocus">
    <xs:attribute name="autofocus" use="required">
      <xs:simpleType>
        <xs:restriction base="xs:token">
          <xs:enumeration value="autofocus"/>
          <xs:enumeration value=""/>
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
  </xs:attributeGroup>
  <xs:attributeGroup name="common.attrs.inputmode">
    <xs:attribute name="inputmode" use="required" type="xs:string"/>
  </xs:attributeGroup>
  <xs:attributeGroup name="common.attrs.nonce">
    <xs:attribute name="nonce" use="required" type="xs:string"/>
  </xs:attributeGroup>
  <!-- ##################################################################### -->
  <xs:simpleType name="common.data.tokens">
    <xs:annotation>
      <xs:documentation> Common Datatypes                                                  #</xs:documentation>
    </xs:annotation>
    <xs:list itemType="xs:token"/>
  </xs:simpleType>
  <xs:simpleType name="common.data.browsing-context">
    <xs:restriction base="xhtml:datatype.html5.browsing.context.name"/>
  </xs:simpleType>
  <xs:simpleType name="common.data.browsing-context-or-keyword">
    <xs:restriction base="xhtml:datatype.html5.browsing.context.name.or.keyword"/>
  </xs:simpleType>
  <!--
    		xsd:string {
    			pattern = "()|([^_].*)|(_[bB][lL][aA][nN][kK])|(_[sS][eE][lL][fF])|(_[pP][aA][rR][eE][nN][tT])|(_[tT][oO][pP])"
    		}
  -->
  <xs:simpleType name="common.data.id">
    <xs:annotation>
      <xs:documentation>IDs and IDREFs</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xhtml:datatype.html5.token"/>
  </xs:simpleType>
  <!--
    		xsd:string {
    			pattern = "\S+"
    		}
  -->
  <xs:simpleType name="common.data.idref">
    <xs:restriction base="xhtml:datatype.html5.token"/>
  </xs:simpleType>
  <xs:simpleType name="common.data.idrefs">
    <xs:restriction base="xhtml:datatype.html5.space.separated.tokens"/>
  </xs:simpleType>
  <xs:simpleType name="common.data.name">
    <xs:restriction base="xhtml:datatype.html5.token"/>
  </xs:simpleType>
  <xs:simpleType name="common.data.hash-name">
    <xs:restriction base="xhtml:datatype.html5.name.reference"/>
  </xs:simpleType>
  <!--
    		xsd:string {
    			pattern = "#.+"
    		}
  -->
  <xs:simpleType name="common.data.integer">
    <xs:annotation>
      <xs:documentation>Numerical</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xhtml:datatype.integer"/>
  </xs:simpleType>
  <!--
    		xsd:string {
    			pattern = "-?[0-9]+"
    		}
  -->
  <xs:simpleType name="common.data.integer.positive">
    <xs:restriction base="xhtml:datatype.positive.integer"/>
  </xs:simpleType>
  <!--
    		xsd:string {
    			pattern = "0*[1-9][0-9]*"
    		}
  -->
  <xs:simpleType name="common.data.integer.non-negative">
    <xs:restriction base="xhtml:datatype.non-negative.integer"/>
  </xs:simpleType>
  <!--
    		xsd:string {
    			pattern = "[0-9]+"
    		}
  -->
  <!--
    	common.data.percent =
    		xsd:string {
    			pattern = "(100)|([1-9]?[0-9](\.[0-9]+)?)%"
    		}
  -->
  <xs:simpleType name="common.data.float">
    <xs:restriction base="xhtml:datatype.float"/>
  </xs:simpleType>
  <!--
    		xsd:string {
    			pattern = "-?[0-9]+(\.[0-9]+)?([eE]-?[0-9]+)?"
    		}
  -->
  <xs:simpleType name="common.data.float.positive">
    <xs:restriction base="xhtml:datatype.float.positive"/>
  </xs:simpleType>
  <!--
    		xsd:string {
    			pattern = "(0*[1-9][0-9]*(\.[0-9]+)?)|([0-9]+(\.0*[1-9][0-9]*)?)([eE]-?[0-9]+)?"
    		}
  -->
  <xs:simpleType name="common.data.float.non-negative">
    <xs:restriction base="xhtml:datatype.float.non-negative"/>
  </xs:simpleType>
  <!--
    		xsd:string {
    			pattern = "[0-9]+(\.[0-9]+)?([eE]-?[0-9]+)?"
    		}
  -->
  <xs:simpleType name="common.data.datetime">
    <xs:annotation>
      <xs:documentation>Temporal</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xhtml:datatype.html5.datetime"/>
  </xs:simpleType>
  <xs:simpleType name="common.data.date">
    <xs:restriction base="xhtml:datatype.html5.date"/>
  </xs:simpleType>
  <xs:simpleType name="common.data.time-datetime">
    <xs:restriction base="xhtml:datatype.html5.date.or.time"/>
  </xs:simpleType>
  <xs:simpleType name="common.data.uri">
    <xs:annotation>
      <xs:documentation>IRIs</xs:documentation>
    </xs:annotation>
    <xs:union memberTypes="xhtml:datatype.IRI">
      <xs:simpleType>
        <xs:restriction base="xs:string">
          <xs:pattern value="[ 
-&#xD;]*"/>
        </xs:restriction>
      </xs:simpleType>
    </xs:union>
  </xs:simpleType>
  <xs:simpleType name="common.data.uri.non-empty">
    <xs:restriction base="xhtml:datatype.IRI"/>
  </xs:simpleType>
  <xs:simpleType name="common.data.uris">
    <xs:list itemType="xhtml:datatype.IRI"/>
  </xs:simpleType>
  <xs:simpleType name="common.data.uris.http.or.https">
    <xs:list itemType="xhtml:datatype.URI.absolute"/>
  </xs:simpleType>
  <xs:simpleType name="common.data.uri.absolute">
    <xs:restriction base="xhtml:datatype.URI.absolute"/>
  </xs:simpleType>
  <xs:simpleType name="common.data.sizes">
    <xs:annotation>
      <xs:documentation>&lt;link type='icon'&gt; sizes</xs:documentation>
    </xs:annotation>
    <xs:restriction>
      <xs:simpleType>
        <xs:list>
          <xs:simpleType>
            <xs:restriction base="xs:string">
              <xs:pattern value="[1-9][0-9]*x[1-9][0-9]*"/>
            </xs:restriction>
          </xs:simpleType>
        </xs:list>
      </xs:simpleType>
      <xs:minLength value="1"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="common.data.color">
    <xs:annotation>
      <xs:documentation>color for &lt;link rel="mask-icon"&gt; and &lt;meta name="theme-color"&gt;</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xhtml:datatype.html5.color"/>
  </xs:simpleType>
  <xs:simpleType name="common.data.mimetype">
    <xs:annotation>
      <xs:documentation>MIME types</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xhtml:datatype.mimetype"/>
  </xs:simpleType>
  <xs:simpleType name="common.data.scripttype">
    <xs:annotation>
      <xs:documentation>Script type</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xhtml:datatype.string"/>
  </xs:simpleType>
  <xs:simpleType name="common.data.charset">
    <xs:annotation>
      <xs:documentation>Encodings</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xhtml:datatype.character.encoding"/>
  </xs:simpleType>
  <xs:simpleType name="common.data.meta-charset">
    <xs:restriction base="xhtml:datatype.string"/>
  </xs:simpleType>
  <xs:simpleType name="common.data.refresh">
    <xs:annotation>
      <xs:documentation>Refresh</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xhtml:datatype.string"/>
  </xs:simpleType>
  <xs:simpleType name="common.data.default-style">
    <xs:annotation>
      <xs:documentation>Default style</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string"/>
  </xs:simpleType>
  <!-- 		w:default-style -->
  <xs:simpleType name="common.data.x-ua-compatible">
    <xs:annotation>
      <xs:documentation>X-UA-Compatible</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string"/>
  </xs:simpleType>
  <xs:simpleType name="common.data.mediaquery">
    <xs:annotation>
      <xs:documentation>Media Queries</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xhtml:datatype.html5.media.query"/>
  </xs:simpleType>
  <xs:simpleType name="common.data.langcode">
    <xs:annotation>
      <xs:documentation>Language Codes</xs:documentation>
    </xs:annotation>
    <xs:union memberTypes="xhtml:datatype.languagecode">
      <xs:simpleType>
