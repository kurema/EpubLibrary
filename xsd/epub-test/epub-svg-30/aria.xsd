    common.attrs.aria.implicit.select |=
    		(	aria.state.invalid?
    	&	aria.prop.required?
    	&	aria.prop.atomic?
    	&	aria.state.busy?
    	&	aria.prop.channel?
    	&	aria.prop.controls?
    	&	aria.prop.live?
    	&	aria.prop.relevant?
    	)
  -->
